  "engines": {
    "node": ">=16.13.0"
  },
  "packageManager": "yarn@3.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ruppyio/emperor-ruppy.git"
  },
  "scripts": {
    "dev": "ts-node --files -r tsconfig-paths/register -r dotenv/config ./src/index.ts",
    "build": "tsc",
