    "emoji-regex": "^9.2.2",
    "pg": "^8.7.1",
    "pretty-ms": "^7.0.1",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^3.11.0",
    "typeorm": "^0.2.40",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@latipun7/eslint-config": "^1.0.4",
    "@types/common-tags": "^1.8.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.11.7",
    "eslint": "^8.4.1",
    "husky": "^7.0.4",
