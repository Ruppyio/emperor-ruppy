    "ts-jest": "^26.5.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "eslintConfig": {
    "extends": "@latipun7"
  },
  "lint-staged": {
    "*.{json,yml,yaml,md,mdx,html,css,sass,scss,graphql}": "prettier --write",
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache",
      "prettier --write"
    ]
  }
